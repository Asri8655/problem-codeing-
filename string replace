#include <stdio.h>
#include<string.h>

int main()
{
    char a[100];
    int i;
    scanf("%s",a);
    
    int x=strlen(a);
    char b[x];
    for(i=0;i<x;i++)
    {
        if(a[i]=='A')
        {
            b[i]='B';
        }else if(a[i]=='a')
        {
            b[i]='@';
        }else if(a[i]=='1')
        {
            b[i]='2';
        }else
        {
            b[i]=a[i];
        }
    }
    printf("%s",a);
    printf("\n%s",b);
    return 0;
}
